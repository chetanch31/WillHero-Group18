            <ImageView fitHeight="480.0" fitWidth="854.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@static/background.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="480.0" prefWidth="854.0">
         <children>
            <ImageView fitHeight="275.0" fitWidth="360.0" layoutX="290.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@static/topScores.png" />
               </image>
            </ImageView>
            <ImageView fx:id="returnButton" fitHeight="121.0" fitWidth="95.0" layoutX="745.0" layoutY="14.0" onMouseClicked="#goHome" pickOnBounds="true" preserveRatio="true">
